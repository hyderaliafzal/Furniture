{
  "name": "Bill",
  "plural": "Bills",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": true
  },
  "properties": {
    "customerName": {
      "type": "string"
    },
    "phoneNumber": {
      "type": "string"
    },
    "discount": {
      "type": "string",
      "default": 0
    },
    "paid": {
      "type": "string"
    },
    "status": {
      "type": "string"
    },
    "subTotal": {
      "type": "string"
    },
    "payment": {
      "type": "string"
    },
    "day": {
      "type": "string"
    },
    "month": {
      "type": "string"
    },
    "year": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "shop": {
      "type": "belongsTo",
      "model": "Shop"
    },
    "products": {
      "type": "embedsMany",
      "model": "billProducts",
      "property": "products",
      "options": {
        "validate": true,
        "forceId": false
      }
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "Employee",
      "permission": "ALLOW",
      "property": ["create", "find"]
    },
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "superAdmin",
      "permission": "ALLOW",
      "property": "*"
    }
  ],
  "methods": {}
}
